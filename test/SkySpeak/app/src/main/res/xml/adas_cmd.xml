<?xml version="1.0" encoding="utf-8"?>
<CmdProvider xmlns:cmd="http://schemas.android.com/apk/res/com.skyworthauto.speak"
    cmd:id="@integer/pid_adas">

    <IntentCmd
        cmd:key="@string/open_adas_id"
        cmd:wakeupWords="@array/open_adas_array"
        cmd:speakWord="@string/open_adas_speak"
        cmd:action="@string/action_control_adas"
        cmd:subAction="@string/ctrl_app"
        cmd:value="@integer/open"/>

    <IntentCmd
        cmd:key="@string/close_adas_id"
        cmd:wakeupWords="@array/close_adas_array"
        cmd:speakWord="@string/close_adas_speak"
        cmd:action="@string/action_control_adas"
        cmd:subAction="@string/ctrl_app"
        cmd:value="@integer/close"/>

    <IntentCmd
        cmd:key="@string/open_early_Warning_id"
        cmd:wakeupWords="@array/open_early_Warning_array"
        cmd:speakWord="@string/open_early_Warning_speak"
        cmd:action="@string/action_control_adas"
        cmd:subAction="@string/ctrl_early_warning"
        cmd:value="@integer/open"/>

    <IntentCmd
        cmd:key="@string/close_early_Warning_id"
        cmd:wakeupWords="@array/close_early_Warning_array"
        cmd:speakWord="@string/close_early_Warning_speak"
        cmd:action="@string/action_control_adas"
        cmd:subAction="@string/ctrl_early_warning"
        cmd:value="@integer/close"/>

</CmdProvider>