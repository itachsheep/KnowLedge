    <IntentCmd
        cmd:key="@string/playback_prev_photo_id"
        cmd:wakeupWords="@array/playback_prev_photo_array"
        cmd:speakWord="@string/playback_prev_photo_speak"
        cmd:action="@string/action_control_dvr"
        cmd:subAction="@string/ctrl_playback_photo"
        cmd:value="@integer/playback_prev"/>

    <IntentCmd
        cmd:key="@string/playback_next_photo_id"
        cmd:wakeupWords="@array/playback_next_photo_array"
        cmd:speakWord="@string/playback_next_photo_speak"
        cmd:action="@string/action_control_dvr"
        cmd:subAction="@string/ctrl_playback_photo"
        cmd:value="@integer/playback_next"/>

</CmdProvider>