<?xml version="1.0" encoding="utf-8"?>
<CmdProvider xmlns:cmd="http://schemas.android.com/apk/res/com.skyworthauto.speak"
    cmd:id="@integer/pid_music">

    <MusicIntentCmd
        cmd:key="@string/media_play_id"
        cmd:wakeupWords="@array/media_play_array"
        cmd:speakWord="@string/media_play_speak"
        cmd:action="@string/action_control_skymusic"
        cmd:subAction="@string/ctrl_music"
        cmd:value="@integer/play"/>

    <MusicIntentCmd
        cmd:key="@string/media_pause_id"
        cmd:wakeupWords="@array/media_pause_array"
        cmd:speakWord="@string/media_pause_speak"
        cmd:action="@string/action_control_skymusic"
        cmd:subAction="@string/ctrl_music"
        cmd:value="@integer/pause"/>

    <MusicIntentCmd
        cmd:key="@string/media_track_prev_id"
        cmd:wakeupWords="@array/media_track_prev_array"
        cmd:speakWord="@string/media_track_prev_speak"
        cmd:action="@string/action_control_skymusic"
        cmd:subAction="@string/ctrl_music"
        cmd:value="@integer/prev"/>

    <MusicIntentCmd
        cmd:key="@string/media_track_next_id"
        cmd:wakeupWords="@array/media_track_next_array"
        cmd:speakWord="@string/media_track_next_speak"
        cmd:action="@string/action_control_skymusic"
        cmd:subAction="@string/ctrl_music"
        cmd:value="@integer/next"/>

    <MusicIntentCmd
        cmd:key="@string/media_repeat_sequence_id"
        cmd:wakeupWords="@array/media_repeat_sequence_array"
        cmd:speakWord="@string/media_repeat_sequence_speak"
        cmd:action="@string/action_control_skymusic"
        cmd:subAction="@string/ctrl_music"
        cmd:value="@integer/repeat_sequence"/>

    <MusicIntentCmd
        cmd:key="@string/media_repeat_single_id"
        cmd:wakeupWords="@array/media_repeat_single_array"
        cmd:speakWord="@string/media_repeat_single_speak"
        cmd:action="@string/action_control_skymusic"
        cmd:subAction="@string/ctrl_music"
        cmd:value="@integer/repeat_single"/>

    <MusicIntentCmd
        cmd:key="@string/media_repeat_random_id"
        cmd:wakeupWords="@array/media_repeat_random_array"
        cmd:speakWord="@string/media_repeat_random_speak"
        cmd:action="@string/action_control_skymusic"
        cmd:subAction="@string/ctrl_music"
        cmd:value="@integer/repeat_random"/>

</CmdProvider>