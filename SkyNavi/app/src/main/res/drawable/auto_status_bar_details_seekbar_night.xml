            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="@dimen/auto_dimen2_8" />
                <solid android:color="@color/auto_color_3197f1" />
            </shape>
        </clip>
    </item>
